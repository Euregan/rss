{
  "name": "rss",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:app": "parcel src/index.html",
    "dev:server": "nodemon --ext ts --exec \"node -r esbuild-register --loader @esbuild-kit/esm-loader server.ts\"",
    "build": "parcel build src/index.html && cp -R src/api dist/api"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "argon2": "^0.30.3",
    "jsonwebtoken": "^9.0.0",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.5.4",
    "@parcel/transformer-elm": "^2.8.2",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.11.18",
    "elm": "^0.19.1-5",
    "esbuild": "^0.16.16",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.2",
    "prisma": "^4.8.1",
    "typescript": "^4.9.4"
  }
}
